/* 	This Program includes an proper overrideing between 
	two a class and its sub-class. 
	This is perfectly legal, so no error. */ 
	
class A {
	
	int func ( int a ) { 
		return a + a ; 
	}

	static void main (string[] args) { 
		int b ; 
		b = 4 ;  
	}
}

class B extends A { 
	
	// overriding of func
	int func ( int a ) {
		return a + a + a ; 
	}
	
}

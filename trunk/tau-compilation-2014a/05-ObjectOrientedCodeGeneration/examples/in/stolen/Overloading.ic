/* 	This Program includes an attempt to overload 
	a superclass method. */ 
	
class A {
	
	int func ( int a ) { 
		return a + a ; 
	}
	
	static void main (string[] args) { 
		int b ; 
		b = func(4) ;  
	}
}

class B extends A { 
	
	// overloading of func
	string func ( string a ) {
		return a + a ; 
	}
	
}
